        "@vue/cli-plugin-unit-jest": "^3.8.0",
        "@vue/cli-service": "^3.6.0",
        "@vue/test-utils": "^1.0.0-beta.20",
        "babel-core": "7.0.0-bridge.0",
        "babel-jest": "^23.0.1",
        "sass-loader": "^7.0.1",
        "vue-template-compiler": "^2.5.17",
        "vuetify-loader": "^1.3.1",
        "sass": "^1.23.6"
    },
    "files": [
        "dist/*",
        "src/*",
        "public/*",
        "*.json",
        "*.js"
    ]
